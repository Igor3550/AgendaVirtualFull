{
  "name": "agendavirtual-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development npx nodemon src/server.ts",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "dev:migrate": "npx prisma migrate dev && npm run dev:seed && npm run start",
    "dev:migrate:envdev": "npm run dev:load-envs prisma migrate dev",
    "dev:prisma:generate": "npm run dev:load-envs prisma generate",
    "dev:load-envs": "dotenv -e .env.development",
    "test": "NODE_ENV=test jest --passWithNoTests --runInBand",
    "test:watch": "npx jest --watch --passWithNoTests --runInBand",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test:load-envs": "dotenv -e .env.test",
    "build": "tsc",
    "start": "tsc && node dist/src/server.js"
  },
  "prisma": {
    "seed": "npm run dev:load-envs -- ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@prisma/client": "^4.9.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "joi": "^17.7.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/http-status": "^1.1.2",
    "@types/jest": "^29.2.6",
    "@types/joi": "^17.2.3",
    "@types/supertest": "^2.0.12",
    "dotenv-cli": "^7.0.0",
    "jest": "^29.3.1",
    "prisma": "^4.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5"
  }
}
